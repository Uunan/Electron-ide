{
  "name": "cortex-editor",
  "version": "1.0.0",
  "description": "Cpanel Editor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "rimraf dist && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "Uunan (https://github.com/Uunan)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Uunan/Electron-ide.git"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "electron-store": "7.0.3",
    "electron-updater": "^6.6.2",
    "form-data": "3.0.1",
    "ssh2-sftp-client": "^12.0.0"
  },
  "devDependencies": {
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "rimraf": "^5.0.7"
  },
  "build": {
    "appId": "com.uunan.ide",
    "productName": "Cortex",
    "files": [
      "main.js",
      "src/**/*",
      "build/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    },
    "publish": {
      "provider": "github",
      "owner": "Uunan",
      "repo": "Electron-ide"
    }
  }
}